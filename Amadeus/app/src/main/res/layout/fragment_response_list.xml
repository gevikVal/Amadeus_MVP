<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/list"
    android:name="fair.com.example.gevik.amadeus.CarListFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical"
    app:layoutManager="LinearLayoutManager"
    tools:context="fair.com.example.gevik.amadeus.presentation.car_list.CarListFragment"
    tools:listitem="@layout/fragment_response">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerView_carList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="0dp"
        android:paddingLeft="-1dp" />

</LinearLayout>
